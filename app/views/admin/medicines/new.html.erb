<h1>New Medicine</h1>

<%= link_to "Back to Dashboard", admin_root_path, class: "btn btn-secondary" %>

<%= form_with model: @medicine, url: admin_medicines_path, local: true do |f| %>
  <%= render "form", f: f %>
  <%= f.submit "Create Medicine", class: "btn btn-success" %>
<% end %>

<%= link_to 'Back to Medicines', admin_medicines_path, class: "btn btn-secondary" %>