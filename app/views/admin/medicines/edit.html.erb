<h1>Edit Medicine</h1>

<%= link_to "Back to Dashboard", admin_root_path, class: "btn btn-secondary" %>

<%= form_with model: @medicine, url: admin_medicine_path(@medicine), local: true do |f| %>
  <%= render "form", f: f %>
  <%= f.submit "Update Medicine", class: "btn btn-primary" %>
<% end %>

<%= link_to 'Back to Medicines', admin_medicines_path, class: "btn btn-secondary" %>